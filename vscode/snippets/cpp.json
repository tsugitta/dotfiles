{
  "cin": {
    "prefix": "ii",
    "body": [
      "cin >> $1;",
      "$0",
    ]
  },
  "cout": {
    "prefix": "oo",
    "body": [
      "cout << $1 << endl;",
      "$0",
    ]
  },
  "cout double": {
    "prefix": "doo",
    "body": [
      "cout << fixed << setprecision(10) << $1 << endl;",
    ]
  },
  "vecmax": {
    "prefix": "vecmax",
    "body": [
      "*max_element($1.begin(), $1.end())",
    ]
  },
  "vecmin": {
    "prefix": "vecmin",
    "body": [
      "*min_element($1.begin(), $1.end())",
    ]
  },
  "vecsum": {
    "prefix": "vecsum",
    "body": [
      "accumulate($1.begin(), $1.end(), 0)",
    ]
  },
  "priority_queue_asc": {
    "prefix": "priority_queue_asc",
    "body": [
      "priority_queue<$1, V<$1>, greater<$1>> $2;",
    ]
  },
}
